<div>
  <div class='col-md-3' align='left'>Mount Olive Pantry</div>
  <div class='col-md-6' align='center'>
    <h5>
      <p>We are here to give you a hand up until you can get back on your feet.</p>
      Located in the lower level of Christ Church</br>
      369 Sandshore Road, Budd Lake, 973-347-1866</br>
    </h5>
  </div>
  <div align='right'>
    <% if current_user %>
      <div class="btn-group">
        <button type="button" class="btn btn-info">
          <%= current_user.name %>
        </button>
        <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
          <span class="caret"></span>
          <span class="sr-only">Toggle Dropdown</span>
        </button>
        <ul class="dropdown-menu dropdown-menu-right" role="menu">
          <li><%= link_to "Edit Account", edit_user_path(current_user) %></li>
          <li><%= link_to "Delete Account", user_path(current_user), method: :delete, data: { confirm: 'Are you sure?' } %></li>
          <li class="divider"></li>
          <li><%= link_to 'Sign Out', session_path, method: :delete %></li>
        </ul>
      </div>
    <% else %>
      <%= link_to 'Become a Volunteer', signup_path, class: 'btn btn-primary' %>
      <%= link_to 'Volunteer Sign In', signin_path, class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>