<h1>Mount Olive Pantry is glad to be a part of the following events:</h1>

<% @events.each do |event| %>
  <article>
    <header>
      <h2><%= link_to event.title, event %></h2>
    </header>
    <p>
      <%= truncate(event.details, length: 40, separator: ' ') %>
    </p>
    <h4>Happening:</h4>
    <p>
       <%= event.date_start %> through <%= event.date_end %>
    </p>
  </article>
<% end %>